    <mj-section background-color="white">
      <mj-column width="400px">
        <mj-image align="center" src="https://codeforphilly.org/img/logo-bell.png" width="150px"></mj-image>
        <mj-text align="center" font-size="24px" color="#525252">Code for Philly Newsletter</mj-text>
      </mj-column>
    </mj-section>

    <mj-section background-color="#f2eded">
      <mj-column width="400px">
        <mj-text align="left" font-size="20px" color="#525252">In This Issue</mj-text>
        <mj-text align="left" color="#525252">
          <mj-list>
            <li>Leadership Team Changes</li>
            <li>Project Profile Series</li>
            <li>Civic Engagement Launchpad 2018</li>
            <li>What's New in Civic Tech</li>
          </mj-list>
